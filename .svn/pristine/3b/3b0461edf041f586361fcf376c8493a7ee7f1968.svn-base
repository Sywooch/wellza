        <script>
            $(document).ready(function () {
                $('.leftmenubutton').click(function () {
                    $('body').toggleClass('showhide-menu');
                });

                function resize()
                {
                    var heights = window.innerHeight;
                    var footerh = $(".inner-footer").height();
                    document.getElementById("inner-content").style.minHeight = heights - footerh + "px";
                }
                resize();
                window.onresize = function () {
                    resize();
                };

                $(window).on("load", function () {
                    $(".customer-side-menu").mCustomScrollbar({
                        axis: "y", // horizontal scrollbar
                        autoHideScrollbar: true

                    });


                });


            });
        </script>
        <script>
            $(document).ready(function ($) {
                //reset
                //  $(".checkbox input").prop("checked", false);

                $(".checkbox input").click(function () {
                    if ($(this).is(":checked")) {
                        //checked
                        $(this).closest('.img-box').addClass("check");
                    } else {
                        //unchecked
                        $(this).closest('.img-box').removeClass("check");
                    }
                });




            });





        </script>
