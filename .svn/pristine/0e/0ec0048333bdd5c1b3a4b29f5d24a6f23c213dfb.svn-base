.manage-profile-modal{
    .modal-content{
        .modal-header{ border: none; margin:0 0 0px; padding: 10px;
        .close{ background-color: #A2A2A2;}
        }
        .modal-title{
            text-align: left; color: $color-666; font-family: $avantgardestd-bk; text-transform: capitalize;  float: left; font-size: 40px;
            @media(max-width:$screen-xs-max){
                font-size: 24px;
            }
        }
        .modal-body{ padding: 0px 30px 30px;
                     @media(max-width:$screen-xs-max){
                         padding: 15px;
                     }
                     .main_heading{ margin-bottom:30px;
                                    #edit_profile{ float: right; margin-bottom:0px; text-decoration: none; color: $color-orange; font-family:$avantgardestd-bk;margin-top:25px;
                                                   &:hover{
                                                       color:lighten($color-orange,10); 
                                                   }
                                                   @media(max-width:$screen-xs-max){
                                                       margin-top:3px;
                                                   }
                                    }
                                    .back_btn{
                                        color: #666666;float: right; margin-bottom: 15px; text-decoration: none; font-family: $avantgardestd-bk; margin-top:25px;
                                        @media(max-width:$screen-xs-max){
                                            margin-top:3px;
                                        }
                                        &:hover{
                                            color:lighten(#666666,10); 
                                        }
                                    }  
                     }

                     .profile-image{
                         width:196px; height:196px; position: relative; border: 3px solid $color-green; padding: 5px; border-radius:50%; margin:auto auto 65px;
                         @media(max-width:$screen-xs-max){
                             width:110px; height:110px;margin:0px auto 30px;
                         } 
                         .edit-btn{
                             position: absolute; top: 15px; right:3px;height: 40px; width: 40px; border-radius: 50%; line-height: 32px; display: block;background-color:#52BFB8;
                             border:3px solid #ffffff;
                             &:hover{ background-color:lighten($color-green , 10);}
                         }
                     } 

                     .bootstrap-select{  
                         .dropdown-toggle{
                             margin-left: 72px;font-family: $avantgardestd-bk; border-radius: 0px; border: none; padding: 0px; font-size: 17px; height: 50px; color:#2c2c2c; text-transform: capitalize;
                         } 
                         &.btn-group.open .dropdown-toggle, .dropdown-toggle:focus{
                             box-shadow: none; background-color: transparent;
                         }

                         .btn-default.active, .btn-default:active, .btn-default:hover, .open > .btn-default.dropdown-toggle{
                             background-color: transparent; box-shadow: none;
                         }
                         & > .dropdown-menu{
                             margin-left: 72px;
                         }
                     }
                     .form-group{position: relative;border-bottom: 2px solid #F7F7F7;
                                 @media(min-width:$screen-sm-min){
                                     margin:0 0 13px; 
                                 }
                                 .control-label{
                                     @media(min-width:$screen-sm-min){
                                         color: #A0A0A0; font-size: 16px; font-weight: normal; position:relative; top:5px; position:absolute; padding:0; margin:0; 
                                     }
                                     @media(max-width:$screen-xs-max){
                                         padding:0px;
                                     }
                                 }

                                 .name{
                                     padding-left: 96px;
                                 }
                                 .gender{
                                     padding-left: 73px;
                                 }
                                 .dob{
                                     padding-left: 89px; 
                                 }
                                 .mob-no{
                                     padding-left: 139px; 
                                 }
                                 .experience{
                                     padding-left: 161px;
                                 }
                                 .address{ 
                                     padding:5px 0px 0px 102px; min-height: 88px;
                                 }
                                 .province{
                                     padding-left: 85px;
                                 }
                                 .city{
                                     padding-left: 42px;
                                 }
                                 .zipcode{
                                     padding-left: 81px;
                                 }
                                 textarea{
                                     resize: none; min-height: 98px;
                                 }
                                 .select-year{
                                     margin-left: 146px; width:auto; position: relative; top: 5px; float: left; margin-right: 10px;    
                                     .year_label{
                                         float: right; margin-left: 5px;
                                     }
                                     @media(max-width:$screen-xs-max){
                                         margin-left:0px;
                                     }
                                 }
                                 .select-month{
                                     width:auto; position: relative; top: 5px;float:left;
                                     .month_label{
                                         float: right;margin-left: 5px;
                                     }
                                 }
                     }

                     .form-control{
                         color: #2c2c2c; border:none;  font-size: 17px; height:40px;
                         @media(max-width:$screen-xs-max){
                             padding-left:0px !important;
                         }
                     } 
                     .edit_profile{
                         .profile-image{ margin-bottom: 25px;
                                         @media(max-width:$screen-xs-max){
                                             margin:0px auto 30px;
                                             .edit-btn{
                                                 right: -17px;
                                             }
                                         } 
                         }
                         .form-group{
                             border:2px solid #F7F7F7;padding-left: 10px; margin-bottom: 10px;
                             @media(max-width:$screen-xs-max){
                                 border:none; padding-left:0px;
                             }
                         }
                         .control-label{top:13px;}
                         .form-control{
                             height: 49px;border:none; color: $color-666; font-size: 17px;
                             @media(max-width:$screen-xs-max){
                                 border:1px solid #F7F7F7; padding-left:10px;
                             }
                         }
                         .gender{ padding-top: 13px;}
                         .address{
                             resize: none; min-height: 98px; padding: 13px 0 0 102px;
                         }
                     }
        }
    }


}


