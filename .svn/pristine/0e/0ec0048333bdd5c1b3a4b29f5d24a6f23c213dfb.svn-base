.wellza-product-checkout-popup{
    .modal-header{
        border:0 !important; padding:18px 42px; margin:0 !important;
        h2{
            font: 45px $avantgardestd-bk; color:$color-666;text-align:left;
            @media(max-width:$screen-xs-max){
                margin-left:28px;
            }
            @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){
                font-size: 30px;
            }
        }
    }
    .modal-body{
        padding:10px 40px !important;
        .form-group{
            margin-top:6px;
            @media(max-width:$screen-xs-max){
                margin-top:0px;margin-bottom:10px;
            }
            label{
                color:$color-666; font: 18px $avantgardestd-bk; font-weight:bold;
                @media(max-width:$screen-xs-max){
                    font-size:14px;
                }
                span{
                    color:#FF0000;
                    @media(max-width:$screen-xs-max){
                        font-size:14px;
                    }
                }
            }
            input{
                height: 60px; border:1px solid #D6D6D6; color:#9F9F9F;padding: 10px 16px;line-height: 1.33333;font-size: 18px;
                @media(max-width:$screen-xs-max){
                    height: 35px; font-size:14px;
                }
                @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){
                    height: 45px;
                }
            }

            .btn-warning{
                text-decoration: none; padding: 15px 30px; font-size:16px; display:inline-block;
            }
        }

        .clr-grn{
            color:$color-green;margin-bottom:18px;
        }
    }
}

