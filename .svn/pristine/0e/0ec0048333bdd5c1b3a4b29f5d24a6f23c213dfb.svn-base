.product-detail-modal{
    .modal-header{
        border:0 !important;
        .close{background:$color-666 !important;}
    }
    .modal-body{
        padding:40px!important; 
        @media(max-width:$screen-xs-max){
            padding:0 !important;
        }
        img{
            @media(max-width:$screen-xs-max){
                width: 150px;margin:0 auto;
            }
            @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){
                width: 220px;margin:0 auto;
            }
        }
        .details{
            position:relative;padding-left: 20px;
             @media(max-width:$screen-sm-max){
                padding-top:20px;
            }
            &:before{
                content: '';display:block;position:absolute;width:1px;height:100%;background:#EEEEEE;left:-17px;
                @media(max-width:$screen-sm-max){
                  content: '';display:block;position:absolute;width:100%;height:1px;background:#EEEEEE;top:20px; left:0;
                }
            }
            h2{
                font: 45px $avantgardestd-bk;color:$color-666;
                @media(max-width:$screen-sm-max){
                   font-size: 20px;
                }
            }
            p{
                font: 32px $avantgarde-md; color:$color-green;
                @media(max-width:$screen-sm-max){
                   font-size: 16px;
                }
            }
            ul{
                font:16px $avantgarde-md;
                @media(max-width:$screen-sm-max){
                   font-size: 14px;padding-right:5px;
                }
                li{
                    color:#9F9F9F;padding-bottom:15px;
                    span{
                        color: $color-666;
                    }
                }
            }
        }
    }
}



