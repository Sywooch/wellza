.refer-client-friend{
  .modal-header{
    border: 0 !important;
  }
  .modal-body{
     background-color:$heading-color;  padding:0 15px 0 15px !important;  position:relative !important;
    .close{
      @extend %cross;
    }
    .refer-left{
      img{
        display: block; margin:0 auto;padding:256px 0;
        @media (max-width:$screen-xs-max){
          padding:25px 0;width:100px;
        }
        @media (min-width: $screen-sm-min) and (max-width: $screen-md-max){
          padding:50px 0;
        }
      }
    }

    .refer-right{
      background-color:$color-white;padding-top:25px;
      .refer-info{
          .heading{
            padding-left:15px;
            h2{
              color: $color-666;font:40px $avantgardestd-bk;
              @media (max-width:480px){
                font-size: 24px;
              }
            }
            p{
              color: #A2A2A2; font:18px  $avantgardestd-bk;
            }
          }

          .refer-form{
                padding-top:25px;
                label{
                    font:18px $avantgardestd-bk; color:$color-666;
                }

                textarea{
                    height:160px;font-size: 18px;
                    @media(max-width:$screen-xs-max){height:110px;font-size: 16px;}
                }
                input{
                    font-size: 18px;
                    @media(max-width:$screen-xs-max){font-size: 16px;}
                }
          }

        }

        .bottom-btn{
            a{
            text-decoration: none; padding:15px 35px; display:inline-block;margin-bottom:25px;margin-top:15px;margin-left:15px;
             @media (max-width:480px){
                 padding:15px;
              }
        }
        
      }
    }

  }
  
} 


