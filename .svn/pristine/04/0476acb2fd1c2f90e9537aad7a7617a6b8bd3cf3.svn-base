header.admin-header{
    font-family: $avantgarde-md;
    .navbar-default {
        height: 80px; background-color: #fff; border: none;border-radius: 0px;margin-bottom: 0px;
        box-shadow: 0px 0px 3px 0px rgba(0, 0, 0, 0.3);
        @media(max-width:$screen-xs-max) {
            height: 50px;
        }
        .navbar-nav > .open > a, .navbar-nav > .open > a:hover, .navbar-nav > .open > a:focus {
            background-color: transparent;
            color: #fff;
        }
        .container-fluid {
            @media(max-width: $screen-xs-max) {
                padding-right: 0px;
            }
        }
        .main-content {
            padding: 0px;
            .left-side {
                width: 250px;  padding: 14px 0px;
                @media(max-width:$screen-xs-max) {
                    width: 180px; padding:0px;
                }
                .logo {
                    width: 174px;
                    @media(max-width:$screen-xs-max) {
                        width: auto; padding: 13px 15px;
                    }
                    img {
                        width: 114px; margin: 0 auto;
                        @media(max-width:$screen-xs-max) {
                            width: 80px;
                        }
                    }
                }
                a {
                    display: inline-block;
                }
                .leftmenubutton {
                    @include transition(left 0.3s ease-in-out); height: 49px; width: 49px;
                    display: inline-block; position: relative; right: -22px;
                    @media(max-width:$screen-xs-max) {
                        height:50px; float:left;left:0;
                    }
                    .icon-bar {
                        height: 3px; width: 22px; background-color: $color-orange; margin-bottom: 4px;display: block;
                        text-align: center;left: 15px; top: 19px; position: relative;
                    }
                }
            }
            .center {
                .main-heading {
                    color: $color-white;
                    margin: 10px 0px;
                }
                h3 {
                    color: #a5a5a5;margin: 0px; padding: 26px 30px;
                    @media(min-width:$screen-sm-min) and (max-width:$screen-md-max){
                         padding:33px 5px; font-size:17px;
                    }
                }
            }
            .navbar-right { padding-right: 20px;
                            @media(max-width:$screen-xs-max) {
                                float: right;padding: 5px 15px 0 0; margin:0px;
                            }
                            & > li {
                                padding: 14px 0;
                                @media(max-width:$screen-xs-max) {
                                    float:left; padding:0px;
                                }
                                & > a {
                                    padding: 11px 15px;font-size: 18px; color: #A5A5A5;
                                    @media(max-width:$screen-xs-max) {
                                        padding:0px;
                                    }
                                    &:hover, &:active, &:focus, &:visited {
                                        color: #A5A5A5;
                                    }
                                    @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max) {
                                        font-size: 14px;
                                    }
                                    @media(max-width:$screen-xs-max) {
                                        font-size: 14px;
                                    }
                                }
                                .dropdown-menu {
                                    width: 100%;
                                    @media(max-width:$screen-xs-max) {
                                     position:absolute; right:0;  top:45px; left:auto; right:0;background-color:$color-white;  box-shadow:0 1px 3px 0 rgba(0, 0, 0, 0.2);
                                     padding:0;
                                    }

                                    li {
                                        a {
                                            padding: 10px;
                                            @media(max-width:$screen-xs-max) {
                                                color:$color-666; font-size:14px; padding:5px 10px;
                                            }
                                        }
                                    }
                                }
                            }

                            .user {
                                padding: 5px 15px;
                                @media(max-width:$screen-xs-max) {
                                    padding:0px;
                                }
                                .user-image {
                                    display: table-cell;vertical-align: middle;
                                    img {
                                        height: 38px;width: 38px;margin-right: 10px; border:2px solid $color-green;
                                    }
                                }
                                .user-name {
                                    display: table-cell;vertical-align: middle;
                                    @media(max-width: $screen-xs-max) {
                                        display:none;
                                    }
                                }
                            }
                            .dropdown-menu {
                                border-radius: 0px;
                                padding: 0px;
                                > li {
                                    > a {
                                        color: #A5A5A5; font-size: 18px;
                                        @media(max-width: $screen-xs-max) {
                                            color: #fff;
                                        }
                                        &:hover {
                                            background: #A5A5A5; color: #fff;
                                        }
                                    }
                                }
                            }
                            .notification-alert {
                                position: relative;font-size: 20px;
                                span.count {
                                    height: 19px;width: 19px;background-color:$color-green;font-size: 11px; color: #fff;position: absolute;left: 32px;top: 7px;border-radius: 50%;
                                    text-align: center; line-height: 19px;
                                    @media(max-width: $screen-xs-max) {
                                        top:2px; left:17px;
                                    } 
                                }
                            }
            }
        }
    }
}