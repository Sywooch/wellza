<script>
    $(document).ready(function ($) {
        //reset
        //  $(".checkbox input").prop("checked", false);

        $(".checkbox input").click(function () {
            if ($(this).is(":checked")) {
                //checked
                $(this).closest('.img-box').addClass("check");
            } else {
                //unchecked
                $(this).closest('.img-box').removeClass("check");
            }
        });




    });





</script>
