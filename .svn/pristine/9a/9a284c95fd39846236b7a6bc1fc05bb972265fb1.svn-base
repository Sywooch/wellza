.service-detail-modal{
    .media_box{ 
        img{
            width: 100%;
        }
    }
    .modal-header{
        padding:0;border:0; 
        button{
            background-color: #f9ae39;  border-radius: 50%;color: #fff;  height: 24px; opacity: 1;position: absolute;  right:8px; top: 8px; width: 24px; position:absolute;z-index: 1;
        }
    }
    .modal-content{
        background:#fff !important; border: 0; position:relative;
        .media_box{
            height: 390px;background-repeat: no-repeat; background-size: contain; background-position: center;
        }
        .product-detail{
            padding:20px 40px; color:#A1A1A1; text-align:justify; font-size:19px;
            .heading{ 
                color: $color-green;font-size:24px; text-align:center;
                @media(max-width:$screen-xs-max){
                    font-size:16px;   
                }
                img{
                    padding:0 15px;
                }
                span{
                    color:#A1A1A1;
                }
            }

            p{ margin-top: 20px;
               @media(max-width:$screen-xs-max){
                   font-size:16px;   
               }
            }
        } 
    }
}
