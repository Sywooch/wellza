.checkout-modal{
    .close{
        background-color: #f9ae39; border-radius: 50%;  color: #ffffff;   height: 24px; opacity: 1;  position: absolute;  right: 6px; top: 6px;width: 24px;
    }
    .modal-body{
        padding:20px 65px 50px !important;
    }
    .payment-section{
        @media(max-width:$screen-xs-max){
            padding:15px;
        }
        .review-section-containet{
            color: #fff; background: #383838;
            h2{
                @media(max-width:$screen-xs-max){
                    font-size:18px; margin:10px 0px;
                } 
            }
        }
        .payment-section-containt{
            @media(max-width:$screen-xs-max){margin-top:50px;}
            @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){margin-top:50px;}
            .containt-top{
                padding: 20px;
                @media(max-width:$screen-xs-max){
                    padding:15px 0px;
                }
                h2{
                    font-family: $avantgardestd-bk;font-size: 39px;color: $color-666;margin-top: 0;
                    @media(max-width:$screen-xs-max){
                        font-size:22px;
                    }
                }
                p{
                    font-family: $avantgardestd-bk; font-size: 20px;color:#A0A0A0;
                    @media(max-width:$screen-xs-max){
                        font-size:18px;
                    }
                }
                .form-group{ 
                    input{
                        height: 50px; border:2px solid #D6D6D6;border-radius:0;
                    }

                    .bootstrap-select{
                        width: 100%;
                        .btn-default{
                            border:2px solid #D6D6D6; color: #A0A0A0; height: 50px; padding-left: 25px; border-radius:0;
                            .bs-caret{
                                .caret{
                                    border-top: 7px dashed;    border-right: 7px solid transparent;    border-left: 7px solid transparent;
                                }
                            }
                        }
                    }

                    label{
                        color: $color-666;font-family: $avantgardestd-bk;font-size: 19px;font-weight: normal;
                        @media(max-width:$screen-xs-max){
                            margin-top: 0px; font-size:18px;
                        }
                    }
                }
                img{
                    margin-top: 25px;
                    @media(min-width:$screen-md-min) and (max-width:1400px){
                        margin: 40px 0;
                    }

                    @media(min-width:1401px) and (max-width:1700px){
                        margin: 40px 0;
                    }
                }
            }

            .container-bottom{
                .pad0{
                    padding:0;
                }
                .save, button.save{
                    display: block;width: 50%;background:$color-orange; color:#fff;text-align: center;display: inline-block;float: left;
                    font-family: $avantgarde-md;font-size: 21px;padding:19px 0;
                    margin-top: 4px;text-decoration: none;
                    &:hover{
                        background:lighten($color-orange, 5%);
                    }
                    @media(max-width:$screen-xs-max){
                        display:block; float:none; width:100%; font-size:18px; padding:15px 5px;
                    }
                } 
                .pay, button.pay{
                    background:$color-green;display: block;width: 50%;color:#fff;text-align: center;display: inline-block; font-family: $avantgarde-md;font-size: 21px;padding:19px 0;margin-top: 4px;
                    text-decoration: none;
                    &:hover{
                        background:lighten($color-green, 5%);
                    }
                    @media(max-width:$screen-xs-max){
                        display:block; float:none; width:100%;font-size:18px;padding:15px 5px;
                    }
                }
            }
        }

    }   
}
