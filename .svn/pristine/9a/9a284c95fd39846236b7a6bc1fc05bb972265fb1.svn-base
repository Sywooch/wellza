.rating-list-modal{
    .close{
        background-color: #f9ae39; border-radius: 50%;  color: #ffffff;   height: 24px; opacity: 1;  position: absolute;  right: 6px; top: 6px;width: 24px;
    }
    .modal-content{ padding:40px 20px; 
                    @media(max-width:$screen-md-max){
                        padding:40px 20px;
                    }
                    @media(max-width:$screen-xs-max){
                        padding:40px 20px;
                    }
                    .modal-body{ padding:0 15px;max-height: 600px; overflow-y: auto;
                                 .feedback-row {
                                     padding: 10px; border:1px solid #f3f3f3; margin-bottom: 5px; 
                                     .review-by{font-size: 12px; text-align: center;color:$color-green;
                                                .rlabel{
                                                    color: #999999;
                                                }

                                     }
                                 }

                                 .reviews {
                                     @media(max-width:$screen-xs-max){
                                         text-align:center;
                                     }
                                     .rating-container { font-size:16px !important;
                                                         .star{
                                                             border-radius: 50%; display: inline-block;font-size: 14px; padding: 1px 4px;height: 21px;width: 21px; line-height: 21px; margin: 0 3px; text-align: center; 
                                                         }  
                                                         .empty-stars{
                                                             color: $color-white;
                                                             -webkit-text-stroke:0px !important; text-shadow: none;
                                                             .star{
                                                                 background-color: $color-666; color: $color-white;
                                                             }
                                                         }
                                                         .filled-stars{
                                                             color: $color-white;-webkit-text-stroke:0px !important; text-shadow: none;
                                                             .star{
                                                                 background-color: $color-orange; color: $color-white;
                                                             }
                                                         }
                                     }
                                     .img-box{
                                         height:60px; width: 60px;  background-size: cover; background-position: center center; border-radius: 50%;
                                     }
                                     .feedback-comment{
                                         margin-top:5px;  color: $color-666; font-size: 14px;
                                     }
                                     .feedback-date{
                                         color:$color-666;font-size: 12px;  
                                         @media(max-width:$screen-xs-max){
                                             float:none !important; margin-top:10px;
                                         }
                                     }

                                 }      
                    }

    }

}