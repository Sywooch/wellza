
    .main-container{
        padding-left:$padding-large*3+25;
    }
    .tabpanel{
        .panel-body{
             @media(max-width:$screen-xs-max){
                   padding:0px;
                 }
            .admintable{
                /*box-shadow: 0 0px 4px 0 rgba(0, 0, 0, 0.2);*/
            }
            .table-container{border-radius:0px; border:none; 
                    min-height: 500px;
                .panel-heading{
                    border-bottom:1px solid #e2e2e2;text-align:right; padding: 15px 0px;
                     @media(max-width:$screen-xs-max){
                            padding:0px; margin-bottom:15px;
                         }
                    .main-heading{ text-align: left;
                       
                        h4{
                         font-size: 45px; margin: 0px; margin-bottom: 8px;font-family: $avantgarde-md; 
                         @media(max-width:$screen-md-max){
                              font-size:30px; 
                         }
                         @media(max-width:$screen-xs-max){
                              font-size:20px; 
                         }
                        }
                        .subline{
                          font-size: 25px; color:$color-666;
                           @media(max-width:$screen-md-max){
                              font-size:18px; 
                         }
                          @media(max-width:$screen-xs-max){
                              font-size:16px; 
                         }
                        }
                         
                    }
                    .search { 
                      display: inline-block; vertical-align: top;
                       @media(max-width:$screen-xs-max){
                            display:block;
                         }
                    .input-group, input {
                        height: 48px; background-color: #F9AE39;border: none;color: #fff;padding-left: 5px;font-family: $avantgarde-md; letter-spacing: 0.3px;
                           @media(max-width:$screen-md-max){
                              height: 35px; width:100%;
                         }
                         @media(max-width:$screen-xs-max){
                               height: 35px;
                         }
                    }
                    .input-group-addon {
                        border: none;background-color: transparent;
                        i {
                            color: #fff;font-size: 18px;
                        }
                    }
                        ::-webkit-input-placeholder{color: #fff; opacity: 0.5;}
                        :-moz-placeholder { /* Firefox 18- */color: #fff; }
                        ::-moz-placeholder {  /* Firefox 19+ */color: #fff; }
                        :-ms-input-placeholder {color: #fff; }
                }
                    .btn-info{ font-family: $avantgarde-md; font-size: 18px; padding: 10px 15px;
                     @media(max-width:$screen-xs-max){
                               font-size: 15px; padding: 6px 15px; display:block;
                         }
                    } 
                   
                }
                .admintable{
                    .table{
                        margin-bottom:0px;
                        th{
                            background-color:$color-green; padding:15px; font-weight:normal;border-bottom:1px solid #dddddd; color: $color-white; font-family: $avantgarde-md; font-size: 16px;
                        }
                        tr{
                            td{
                                border-top:0px; vertical-align:middle; padding: 15px;font-family: $avantgarde-md; color:$color-666; background-color: $color-white;
                                border-bottom:1px solid #e8e8e8;
                                .action{
                                    @extend .list-inline;margin-bottom:0px;
                                    li{
                                        vertical-align:middle;
                                        a{
                                            font-size:20px;
                                            &.view{
                                                color:#F9AE39;
                                            }
                                            &.edit{
                                                color:$color-green;
                                            }
                                            &.delete{
                                                color:#000;
                                            }
                                        }
                                        .toggle.ios, .toggle-on.ios, .toggle-off.ios { border-radius: 20px; }
                                        .toggle.ios .toggle-handle { border-radius: 20px; }
                                    }
                                }
                            }
/*                            &:nth-child(odd){
                                background-color:$color-white;
                            }
                            &:nth-child(even){
                                background-color:#f1f1f1;
                            }*/
                        }
                    } 
                }
                .loadmore {
                    font-size:$font-size-large;margin:$margin-medium auto $margin-medium;padding:$padding-xs-small 0;text-align: center;border:1px solid $panel-inner-border;
                    width:150px;background-color:$gray-lighter;
                    a {
                        color:#A6A6A6; text-decoration:none;
                    }
                }
            }
        }
    }
