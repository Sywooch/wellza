/*========inner-pages-css=============*/
.inner-content{
    background-color: #F9F9F9; padding-top: 90px; padding-bottom:20px;
    @media(max-width:$screen-xs-max){
        padding-top: 65px;
    }
    .btn-bottom{
        margin-top: 40px;
        @media (min-width:$screen-lg-min) and (max-width:$screen-lg-max){
            margin-top: 20px;
        }
        @media (min-width:$screen-sm-min) and (max-width:$screen-md-max){
            margin-top: 20px;
        }
        @media(max-width:$screen-xs-max){
            margin-top: 0px; margin-bottom:0px;
        }
        .btn{
            letter-spacing: 0.7px; padding:15px 25px;
            @media (min-width:$screen-sm-min) and (max-width:$screen-lg-max){
                padding:10px 20px; margin-top: 20px;
            }
            @media (max-width:$screen-xs-max){
                padding:8px 10px; font-size:14px;
            }
        }
    }
}
.inner-heading{
    font-size:38px; color: $color-666; font-family: $avantgarde-md;  text-align: left; margin: 0px; padding: 30px 0px; 
    @media (min-width:$screen-lg-min) and (max-width:$screen-lg-max){
        font-size:35px;  
    }
    @media (min-width:$screen-sm-min) and (max-width:$screen-md-max){
        font-size:30px; 
    }
    @media (max-width:$screen-xs-max){
        font-size:22px; padding: 10px 0px 10px; 
    }

}
.inner-subheading{
    font-size:24px; color:$color-666;text-align: center;font-family: $avantgardestd-bk; 
    @media (min-width:$screen-lg-min) and (max-width:$screen-lg-max){
        font-size:22px; 
    }
    @media (min-width:$screen-sm-min) and (max-width:$screen-md-max){
        font-size:18px; 
    }
    @media (max-width:$screen-xs-max){
        font-size:14px; 
    }
}

/*========end inner-pages-css=============*/

/*========inner-header-css=============*/
header.inner-header{
    font-family: $avantgarde-md; 
    .navbar-collapse.collapse {
        display: block!important;
    }

    .navbar-nav>li, .navbar-nav {
        float: left !important;
    }
    .navbar-right { 
        float: right!important;
    }
    .navbar-default {
        height: 90px; background-color: #fff; border: none;border-radius: 0px;margin-bottom: 0px;
        box-shadow: 0px 0px 3px 0px rgba(0, 0, 0, 0.3);
        @media(max-width:$screen-xs-max){
            height:50px;
        }
        .navbar-nav > .open > a, .navbar-nav > .open > a:hover, .navbar-nav > .open > a:focus {
            background-color: transparent;
            color: #fff;
        }

        .main-content {
            padding: 0px;
            .left-side {
                padding: 14px 0px;
                @media(max-width:$screen-xs-max){
                    padding:12px 0px; 
                }
                .logo {
                    img {
                        margin: 0 auto;
                        @media(max-width:$screen-xs-max){
                            width:80px; 
                        }
                    }
                }
                a {
                    display: inline-block;
                }

            }
            .navbar-right {
                @media(max-width: $screen-xs-max) {
                    margin: 0px;
                }
                > li {
                    padding: 21px 0;
                    @media(max-width:$screen-xs-max){
                        padding:0px; position:initial; 
                    }
                    > a {
                        padding: 11px 15px;font-size: 18px; color: #A5A5A5;
                        &:hover, &:active, &:focus, &:visited {
                            color: #A5A5A5;
                        }
                        @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max) {
                            font-size: 14px;
                        }
                        @media(max-width:$screen-xs-max) {
                            font-size: 14px;
                        }
                    }
                }
                .dropdown{position: relative;
                          @media(max-width:$screen-xs-max) {
                              position: static;
                          }
                          .dropdown-menu {
                              display: block;visibility: hidden; opacity: 0; position: absolute; top: 100%; width: 100%;  transform: translateY(2em);
                              transition: all 0.3s ease-in-out 0s, visibility 0s linear 0.3s, z-index 0s linear 0.01s;

                              @media(max-width:$screen-xs-max) {
                                  position:absolute;background-color:$color-white;
                              }
                              li {
                                  a {
                                      padding: 10px;
                                      @media(max-width:$screen-xs-max) {
                                          color:#666;
                                      }
                                  }
                              }
                          }
                          &:hover .dropdown-menu{
                              visibility: visible; /* shows sub-menu */ opacity: 1;
                              z-index: 1;  transform: translateY(0%);
                              transition-delay: 0s, 0s, 0.3s; /* this removes the transition delay so the menu will be visible while the other styles transition */
                          }
                }
                .leftmenubutton {
                    @include transition(left 0.3s ease-in-out);
                    display: inline-block; position: relative; margin-top: 5px;
                    .icon-bar { 
                        height: 3px; width: 22px; background-color: $color-666; margin-bottom: 4px;display: block;
                        text-align: center;position: relative;
                    }
                }
                .cart{
                    span{
                        display: table-cell;vertical-align: middle; position: relative;
                    }
                    .count{
                        height: 22px; width: 22px; line-height: 22px; position: absolute; background-color: $color-green; color: $color-white; border-radius: 50%; text-align: center; top: 0; 
                        font-size: 15px;font-family: $avantgarde-md; left: -8px; line-height:22px;
                    }
                    .dropdown-menu{
                        border-radius: 10px; width: 508px; margin-top: 15px;
                        @media(max-width:$screen-xs-max){
                            width:100%;
                        }
                        &:after{
                            content: ''; position: absolute;  width: 0;   height: 0; border-left: 10px solid transparent;  border-right: 10px solid transparent;border-bottom: 10px solid #F9AE39;
                            top: -10px; right: 14px;
                            @media(max-width:$screen-xs-max){
                                right:125px;
                            }
                        }   
                        .cartitems{
                            height: 400px; overflow: auto;
                            .mCSB_container{
                                margin-right: 0px;
                            }
                        }
                        p{
                            white-space: normal;
                        }
                        .cart-heading{ 
                            background-color: #F9AE39; padding: 20px 20px; color:$color-white;font-family: $avantgarde-md; border-radius: 10px 10px 0px 0px;
                            @media(max-width:$screen-xs-max){
                                padding:10px;
                            }
                            h4{
                                font-size: 20px; margin:0px; letter-spacing: 0.3px;
                                @media(max-width:$screen-xs-max){
                                    font-size: 16px;
                                }
                            }

                        }

                        .no-item-cart{
                            padding:50px 11em;text-align:center;
                            @media(max-width:$screen-xs-max){
                                padding:30px 100px;
                            }

                            .zero-item{
                                position:relative;border-bottom:2px solid #EDEDED;margin-bottom:50px;
                                @media(max-width:$screen-xs-max){
                                    margin-bottom:15px; display:inline-block;padding: 0 22px;
                                }
                                .zero{
                                    position:absolute;left:35px; background:$color-green; @include border-radius (50);width:35px; height:35px;
                                    @media(max-width:$screen-xs-max){left:5px;}
                                    span{
                                        text-align: center;color:$color-white;font-size:25px;display:inherit;
                                    }
                                }
                                img{
                                    @media(max-width:$screen-xs-max){
                                        width: 80px;
                                    }
                                }
                            }

                            .cart-msg{
                                h2{
                                    color:#A0A0A0;
                                    @media(max-width:$screen-xs-max){
                                        font-size: 22px;
                                    }
                                    span{
                                        color:$color-green;display:inherit;
                                    }
                                }
                            }

                        }

                        .cart-loadmore{text-align: center; @extend .clearfix;
                                       a{
                                           font-size: 18px; padding: 5px;  text-decoration: none; color: #A2A2A2;  display: block; 
                                       }
                        }
                        .checkout-btn{
                            padding: 10px;text-align:right; 
                        }
                        li{border-bottom:1px solid #EEEEEE;padding: 20px 20px; 
                           @media(max-width:$screen-xs-max){
                               padding: 10px; 
                           }
                           &:last-child{
                               border-bottom:none;
                           }
                           &:hover{
                               a{
                                   background-color: transparent;  
                               } 
                           }
                           a{ padding: 0px;
                              h4{
                                  font-size: 20px; color: $color-green;font-family: $avantgarde-md;  margin-top: 0px;
                                  @media(max-width:$screen-xs-max){
                                      font-size: 16px; 
                                  }
                              }
                              .package-detail, .delete{
                                  display: table-cell; vertical-align: middle;  width: 100%;
                              }
                              p{
                                  font-size: 16px; color:#A0A0A0; font-family: $avantgarde-md; 
                                  @media(max-width:$screen-xs-max){
                                      font-size: 14px; line-height:1.3;
                                  }
                              }
                              .price{
                                  font-size: 22px; color: $color-orange; 
                                  @media(max-width:$screen-xs-max){
                                      padding: 19px;  padding:0px;font-size: 18px;
                                  }
                              }

                              .delete-btn{
                                  border-radius: 50%; height:50px; width: 50px; background-color: #F9F9F9; border: 1px solid #F0F0F0; padding: 0px; text-align: center;
                                  &:hover,&:active, &:focus {
                                      border: 1px solid $color-green;
                                  }
                              }


                           }

                        }
                    }
                }
                .notification{
                    a{
                        span{
                            display: table-cell;vertical-align: middle; position: relative;
                        }
                        .count{
                            height: 22px; width: 22px;  line-height: 22px;position: absolute; background-color: $color-green; color: $color-white; border-radius: 50%; text-align: center; top: 0; 
                            font-size: 15px;font-family: $avantgarde-md; left: -8px; line-height:22px;
                        }
                    }
                    .dropdown-menu{
                        border-radius: 10px; width: 508px; margin-top: 15px;
                        @media(max-width:$screen-xs-max){
                            width:auto;
                        }
                        &:after{
                            content: ''; position: absolute;  width: 0;   height: 0; border-left: 10px solid transparent;  border-right: 10px solid transparent;border-bottom: 10px solid #52BFB8;
                            top: -10px; right: 14px;
                            @media(max-width:$screen-xs-max){
                                right:84px;
                            }
                        }   
                        p{
                            white-space: normal;
                        }
                        .cart-heading{ 
                            background-color: #52BFB8; padding: 20px 20px; color:$color-white;font-family: $avantgarde-md; border-radius: 10px 10px 0px 0px; text-transform: uppercase;
                            @media(max-width:$screen-xs-max){
                                padding:10px;
                            }
                            h4,.count{ display:table-cell; vertical-align: middle;}
                            h4{
                                font-size: 20px; margin:0px; letter-spacing: 0.3px; padding-right:15px;
                                @media(max-width:$screen-xs-max){
                                    font-size: 16px;
                                }
                            }
                            .bootstrap-switch{
                                border-radius: 22px; width: 60px !important; border: none;
                                .bootstrap-switch-container{
                                    background:$color-orange; width:90px !important;
                                    .bootstrap-switch-primary{background:transparent;padding:0; position:relative; top:-1px;padding-right:5px;} 
                                    .bootstrap-switch-label{width: 20px !important; height: 20px; border-radius: 50%; position:relative;left:2px; height:23px; top:1px; @include box-shadow(0,0,5px,rgba(#000, 0.5))}
                                    .bootstrap-switch-default{background:transparent; color:$color-white;padding:0; position:relative; top:-1px;padding-left:5px;}
                                }
                            }


                            .count{
                                height: 45px; width: 48px; border-radius: 50%; border: 2px solid $color-white; line-height: 45px; text-align: center; font-size: 20px;
                            }

                        }
                        .loadmore{text-align: center; @extend .clearfix;
                                  a{
                                      font-size: 18px; text-decoration: none; color: #A2A2A2;  display: inline-block;
                                  }
                        }
                        li{
                            @media(max-width:$screen-xs-max){
                                padding: 10px; 
                            }
                            &:last-child{
                                border-bottom:none;
                                .notification-box{
                                    border-bottom: none;
                                }
                            }
                            &:hover{
                                a{
                                    background-color: transparent;  
                                } 
                            }
                            a{ padding: 0px; text-decoration: none;
                               h4{
                                   font-size: 18px; color: #F8AC39;font-family: $avantgarde-md;  margin:0 0 3px;
                                   @media(max-width:$screen-xs-max){
                                       font-size: 16px; 
                                   }
                               }
                            }
                            .date-time{
                                color: #A0A0A0; font-size: 14px;
                            }
                            .detail, .icon{
                                display: table-cell; vertical-align: top; 
                            }
                            .notification-box{border-bottom: 2px solid #EEEEEE; padding:15px;
                                              .detail{ padding-left: 15px;
                                                       p{
                                                           font-size: 14px; color:$color-666; font-family: $avantgarde-md;  margin:10px 0 0px;;
                                                           @media(max-width:$screen-xs-max){
                                                               font-size: 14px; line-height:1.3;
                                                           }
                                                       }  
                                                       .read-more{
                                                           color:#A2A2A2; font-size: 14px;
                                                       }
                                              } 
                            }


                        }
                    }
                }
                .dropdown-toggle {
                    padding: 5px 10px;
                    @media(max-width:$screen-xs-max){
                        padding:5px;
                    }

                    .user-image {
                        display: table-cell;
                        vertical-align: middle;
                        img {
                            height: 38px;width: 38px;margin-right: 10px; border:2px solid $color-green;
                        }
                    }
                    .user-name {
                        display: table-cell;vertical-align: middle;
                    }
                }
                .dropdown-menu {
                    border-radius: 0px; padding: 0px; box-shadow: 0px 0px 2px 0px rgba(0, 0, 0, 0.19); border: none;
                    > li {
                        > a {
                            color: #A5A5A5; font-size: 18px;
                            @media(max-width: $screen-xs-max) {
                                color: #fff;
                            }
                            &:hover {
                                background: #A5A5A5; color: #fff;
                            }
                        }
                    }
                }

            }
        }
    }
}
