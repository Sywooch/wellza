                   }
                   @media(max-width:$screen-xs-max){
                   }

                   li{
                       a{
                           font-size: 18px; color: $color-white; text-decoration: none; text-align: center; padding: 5px 15px; letter-spacing: 0.3px;
                           @media(max-width:$screen-xs-max){
                               font-size: 14px; padding: 5px 8px; 
                           }
                           @media (min-width:$screen-sm-min) and (max-width:$screen-md-max){
                               font-size: 16px; padding: 10px 8px; 
                           }
                       }
                   }
    }
    .copyright{
        font-size: 14px; color: #929292; text-align: center; background-color: #3F3F3F; padding:10px;
        @media(max-width:$screen-xs-max){
            padding:15px; font-size:12px;
        }
    }
}

/*========end-inner-footer-css=============*/



/*========customer-sidebar-css=============*/
.showhide-menu{
    .customer-side-menu{ 
        right:0px;
    }
}
.customer-side-menu{
    color: #FFFFFF; @include transition(right 0.3s ease); z-index:99999; overflow: hidden;  font-size: 12px;  font-weight: 200;   position: fixed;  top: 0px;  width: 275px; right:-275px; height: 100%; 
    @media(max-width:$screen-xs-max){
        top:0px;
    }  
    .orange_box{
        background-color:$color-orange; padding: 15px; height: 90px; position: absolute; width: 100%; z-index: 9999;
        @media(max-width:$screen-xs-max){
            height: 50px;
        }
        a{
            display: inline-block; position: relative; top: 17px; left: 9px;
            @media(max-width:$screen-xs-max){
                top: -2px;
            }
        }
    }
    .side-menu-content{
        background-color:#52BFB8; padding-top: 90px;
        @media(max-width:$screen-xs-max){
            padding-top: 50px;
        }
        .menu-list{
            overflow-y:auto;
            ul{
                .plus-icio{
                    &:after{
                        content: ''; display:block; background-image:url(../images/plus-white.png); background-repeat:no-repeat; background-position:right;width:17px; height:17px;position: absolute;left:235px;top: 25px; background-color:transparent;
                    }
                }

                .arrow-white{
                    &:after{
                        content: ''; display:block; background-image:url(../images/minus-white.png); background-repeat:no-repeat; background-position:right;width:17px; height:17px;position: absolute;left:235px;top: 25px; background-color:transparent;
                    }
                }

                li{
                    padding:10px 0 11px 59px;  position: relative;line-height: 35px;  border-left: 3px solid $color-green;  border-bottom: 1px solid #39938C; 
                    &:before{
                        height: 1px; content: ''; position: absolute; bottom: -1.5px; background-color: #77CEC8; display: block; width: 100%; left: 0;
                    }
                    &:hover{ background-color: #43B6AD; @include transition(background-color 0.3s ease); }


                    a { text-decoration: none;  color: #ffffff;  font-size: 16px;  font-family: $avantgardestd-bk;  letter-spacing: 0.4px;
                        padding: 7px 0px;  display:block; position:relative;

                    }
                    &.active{background-color: #43B6AD; color:$color-white;}
                    &.icon {background-image: url('../images/icon-sidemenu.png'); background-repeat: no-repeat;}
                    &.profile{  background-position: 15px 21px;}
                    &.appointment{ background-position: 15px -49px;}
                    &.product{ background-position: 15px -120px;}
                    &.notification{ background-position: 15px -195px;
                                    a{
                                        .count{
                                            height: 26px; width: 26px; display: inline-block;  background-color: #F9AE39; border-radius: 50%; vertical-align: middle;
                                            text-align: center; line-height: 26px; margin-left: 15px;
                                        }
                                    }
                    }
                    &.inbox{ background-position: 15px -268px;}
                    &.favourite{ background-position: 15px -339px;}
                    &.blog{ background-position: 15px -415px;}
                    &.membership{ background-position: 15px -484px;}
                    &.referral{ background-position: 15px -561px;}
                    &.aboutus{ background-position: 15px -631px;} 

                }
                .sub-menu { 
                    li {  padding:0px; background-image: url('../images/submenu_icon.png');background-repeat: no-repeat;  background-position: 32px center;
                          a{
                              padding:5px 0px 5px 59px;    
                          }

                    } 
                }
            }
        }
    }

    .mCSB_inside > .mCSB_container {margin-right: 0px;}
    .mCSB_scrollTools{width: 2px;}
    ul :not(collapsed) .arrow:before, li :not(collapsed) .arrow:before {
        font-family: FontAwesome;  content: "\f078";  display: inline-block;  padding-left: 10px;  padding-right: 10px;  vertical-align: middle;  float: right;
    }

}
.body-overlay{ 
    display:none; background:rgba(0,0,0,0.5); filter: alpha(opacity = 50); /* IE7 & 8 */  position:fixed; right: 0; top: 0; width:100%; height:100%; z-index:9999;
}

/*========end-customer-sidebar-css=============*/ 

/*============= modal-css ========================*/
.inner-modal{ background-color: rgba(0,0,0,0.5);
              .modal-lg{
                  @media(min-width:$screen-lg-min){
                      width: 1011px;
