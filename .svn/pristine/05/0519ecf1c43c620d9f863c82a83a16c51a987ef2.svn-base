.container01 {
    @include container-fixed;

    @media (min-width: $screen-sm-min) { 
        width: $container-sm01;
    }
    @media (min-width: $screen-md-min) {
        width: $container-md01;
    }
    @media (min-width: $screen-lg-min) {
        width: $container-lg01;
    }
    @media(min-width:$screen-lg-max) {
        width:1500px;
    }  
    @media(min-width:$screen-xlg-max) {
        width:1666px;
    }  
} 

.container{ 
    @media(min-width:1400px) and (max-width:1699px){
        width:1300px !important;
    }  
    @media(min-width:$screen-xlg-max){
        width:1450px;
    }  
}  
.btn-primary{
    background-color: #666666; color: $color-white; font-size: 16px; border-radius: 0px;
    &:hover, &:focus, &:active{
        background-color: lighten(#666666,10%) !important; border-color:lighten(#666666,10%) !important; color: $color-white !important;
    }
}
.btn-info{
    background-color: $color-green !important;border-color:$color-green !important;  color: $color-white !important; font-size: 16px; border-radius: 0px;
    &:hover, &:focus, &:active{
        background-color: #5bc8c1 !important; border-color:$color-green !important; color: $color-white !important;
    }
}

.btn-warning{
    background-color: #F9AE39;  border-color:#F9AE39 !important; color: $color-white !important; font-size: 16px; border-radius: 0px;
    &:hover, &:focus, &:active{
        background-color:#f4bc47 !important; border-color:#f4bc47 !important; color: $color-white !important;
    }
}

.btn-danger{
    background-color: transparent; border: 2px solid $color-white; padding: 10px 15px; color: $color-white; font-size: 16px; border-radius: 0px;
    &:hover, &:focus, &:active, &:visited{
        background-color: $color-yellow !important; border-color:$color-yellow !important; color: $color-white !important;
    }
}

.loadmore{
    font-size: 18px; color: #A2A2A2;  display: inline-block; text-align: center; @extend .clearfix; margin-top: 15px;
    &:hover, &:focus, &:active{
        color: #9c9c9c; text-decoration: none;
    }
}
.color-666{ color:$color-666;}
.color-orange{ color: $color-orange;}
.color-green{ color: $color-green;}
.bg-orange{ background-color: #F9AE39;}
.bg-gray{ background-color: #666666;}
.bg-green{ background-color: #52BFB8;}

/*========for star rating icons=============*/
.rating-container { font-size:16px !important;
                    .star{
                        border-radius: 50%; display: inline-block;font-size: 14px; height:21px; width:21px; line-height:21px; margin: 0 4px 0px 4px; padding: 1px 4px; text-align: center; 
                    }  
                    .empty-stars{
                        color: $color-white;
                        -webkit-text-stroke:0px !important; text-shadow: none;
                        .star{
                            background-color: $color-666; color: $color-white;
                        }
                    }
                    .filled-stars{
                        color: $color-white;-webkit-text-stroke:0px !important; text-shadow: none;
                        .star{
                            background-color: $color-orange; color: $color-white;
                        }
                    }
}  


/*========All bootstrap select hover effect disable=============*/
.bootstrap-select.btn-group{
    .btn-default{border-color:#eaeaea;
                 &.active,&:active,&:hover,&.focus,&:focus{
                     background-color: transparent;border-color:#eaeaea;
                 }
    }
    .dropdown-toggle{
        &:focus{
            outline:none !important; 
        }
    }
    &.open {
        .dropdown-toggle{ box-shadow: none;border-color:#eaeaea;background-color: transparent;
                          &.focus,&:focus,&.active,&:active,&:hover {background-color: transparent; border-color:#eaeaea; }
                          &.active{
                              &:hover{
                                  background-color: transparent; border-color:#eaeaea;   
                              }
                          }
        } 
    }
}

