.index-menu-btn{
    .leftmenubutton{
        position:fixed;display:block; right:0; right:150px; top:50px; z-index:999999;
        @media(max-width:$screen-xs-max){right:25px;}
        &:before{content:'MENU';position:absolute;left: -64px;top: -5px;color: $color-orange;font-size:20px;}
        .icon-bar{
            height: 3px;width: 22px; background-color: $color-orange;margin-bottom: 5px;display: block; text-align: center; 
        }
    }
}
.showhide-menu{
    .side-menu-home{ 
        right:0px;
    }
}
.side-menu-home{
    color: #FFFFFF;  @include transition(right 0.3s ease);overflow: hidden;  font-size: 12px;  font-weight: 200;  position: fixed;  top: 0px;  width: 275px; right:-275px; height: 100%; 
    background:rgba(#000,.8);top: 0px;padding-top: 80px; z-index: 9999;

    .menu-list{
        overflow-y:auto;
        ul{
            li{
                padding:5px 3px 0px 20px;position: relative;line-height: 35px;
                &:after{
                    height: 1px; content: ''; position: absolute; bottom: -1.5px;  background:transparent; display: block; width: 100%; left: 0;
                }
                a { text-decoration: none;  color: #ffffff;  font-size: 16px;  font-family: $avantgardestd-bk;  letter-spacing: 0.4px;
                    padding: 7px 0px;  display:block;
                    span{
                        &.icon{ width:23px; height: 22px; display: inline-block; margin-right: 10px; background-image: url('#{$imageurl}/icon-index-menu.png');background-repeat: no-repeat; vertical-align: middle; 
                                &.login{
                                    background-position:0 0;
                                }
                                &.signup{
                                    background-position:0px -258px;
                                }
                                &.manage-profile{
                                    background-position:38px 24px;
                                }
                                &.blog{
                                    background-position:0 -50px
                                }
                                &.membership{
                                    background-position:0 -223px;
                                }
                                &.referral{
                                    background-position:38px -128px;
                                }
                                &.aboutus{ background-position: 0px -101px;} 

                                &.product{
                                    background-position: 0px -187px; 
                                }
                        }

                    }
                    &:hover{
                        color:$color-orange;@include transition(background-color 0.3s ease);
                        span{
                            &.icon{background-image: url('#{$imageurl}/icon-index-menu-orange.png'); }
                        }
                    }
                    &.active{background-color: #43B6AD; color:$color-white;}
                }
            }
        }
    }
}

/*============End Home page Navigation================*/


