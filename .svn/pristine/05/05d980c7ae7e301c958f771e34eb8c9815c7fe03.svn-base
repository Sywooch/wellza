/*============Footer===============*/
#footer{ background-color: $heading-color; padding: 15px; color:$color-white;position:absolute;width:100%;//bottom:-17px;
         .footer-left-wrap{
             @media(max-width:$screen-xs-max){
                 text-align: center;
             }
             .footer-logo{
                 img{
                     width: 70px;margin-right:20px;float:left;
                     @media(max-width:$screen-xs-max){
                         text-align: center;margin: 0 auto 20px;float:none;
                     }
                 }
             }

             .footer-text-link{
                 margin-top:16px;float:left;
                 @media(max-width:$screen-xs-max){
                     float:none;
                 }
                 p{
                     &:last-child{
                         font-size: 12px;
                     }
                     a{
                         margin-left: 5px;
                         img{
                             margin-top: -10px;
                         }
                     }
                 }
             }
         }


         .social-media{margin-top:20px;
                       @media(max-width:$screen-xs-max){
                           text-align: center;margin-top:40px;
                       }
                       li{
                           a{
                               height: 45px;width: 45px; background-color: $color-green; color:$color-white; border-radius: 50%;
                               display: table-cell; vertical-align: middle; text-align: center; font-size: 22px;

                               &:hover{
                                   background:#419691;
                               }
                           }
                       }
         }
         .footer-links{ text-align: center; margin-top: 40px;
                        @media (min-width:$screen-sm-min) and (max-width:$screen-lg-max){
                            margin-top: 0px;
                        }
                        @media(max-width:$screen-xs-max){
                            margin-top: 20px;
                        }

                        li{
                            a{
                                font-size: 18px; color: $color-white; text-decoration: none; text-align: center; padding: 5px 15px; letter-spacing: 0.3px;
                                @media(max-width:$screen-xs-max){
                                    font-size: 14px; padding: 5px 8px; 
                                }
                                @media (min-width:$screen-sm-min) and (max-width:$screen-md-max){
                                    font-size: 16px; padding: 10px 8px; 
                                }
                            }
                        }
         }
         .copyright{
             font-size: 14px; color: #929292; text-align: center; background-color: #3F3F3F; padding:25px; margin-top: 25px;
             @media(max-width:$screen-xs-max){
                 padding:15px; font-size:12px;
             }
         }

}

/*============End Footer===============*/