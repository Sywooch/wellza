            }
            @media(max-width:$screen-xs-max){
                text-align: center;
            }
            img{
                position: relative;
                @media(min-width:$screen-sm-min) and (max-width:$screen-lg-max){
                    width:300px;
                }

                @media(max-width:$screen-xs-max){
                    width:230px; margin-top:15px;
                }
            }
        }
    }

}
/*============xxxxxxxxxxxxxxxxx===============*/

/*============xxxxxxxxxxxxxxxxx===============*/
.last-screen{ 
    min-height: 850px; background:#FCFCFC;
    @media(min-width:$screen-sm-min) and (max-width:$screen-md-max){
        padding:40px 0px;min-height:auto;
    }
    @media (max-width:$screen-xs-max){
        min-height:auto;
    }
    .getintouch-logo{
        padding-top:3em;
        .logo-slider{
            padding-top:3em;padding-bottom:3em;
        }
    }
    h2{
        font-size: 40px; text-align: center; color:$heading-color; font-family: $avantgarde-md; margin-top: 0px;    
        @media (max-width:$screen-xs-max){
            font-size: 24px; margin-bottom:15px; text-align:center;
        }
    }
    p{
        color:$color-666;padding:20px 0;
    }

    .email-signup{
        border-top:1px solid #F3F3F3;padding-top:10em;padding-bottom:10em;
        @media(max-width:$screen-xs-max){
            padding-top:6em;padding-bottom:6em;
        }
        @media(max-width:480px){
            padding-top:7em;padding-bottom:7em;
        }
        p{
            font:21px $avantgardestd-bk; color:$color-666;padding:25px 0;
        }

        .form-group{
            width:500px; margin:0 auto;
            @media(max-width:480px){
                width:100%;
            }
            input{
                border-radius:0;height:50px;width: 300px;float:left;background:#fff;box-shadow: 0 0 0 0; border:0;
                @media(max-width:480px){
                    width: 50%; margin-left:5%;
                }
            }
            .btn-info{
                padding: 14px 50px;float: left;height: 49px; margin-top: 1px; margin-left: 35px;  text-decoration: none;
                @media(max-width:480px){
                    width: 30%; margin-left:0;padding:15px;
                }
            }
        }
    }

} 
