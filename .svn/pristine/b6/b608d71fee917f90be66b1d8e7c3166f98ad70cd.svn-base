.sp-apponintment-history-section{
    .inner-heading{
        text-align: left; font-size: 25px;
    } 
    .wellzabundle-tabs{margin-top: 20px;
                       .btn-bottom{
                           margin-bottom: 75px;
                           @media(max-width:$screen-xs-max){
                               margin-bottom: 20px;
                           } 
                       }
                       ul{  box-shadow: 0 0 4px -1px rgba(0,0,0,0.2); border: none; background-color: $color-white;
                            @media(max-width:$screen-xs-max){
                                overflow-x: auto; overflow-y: hidden; white-space: nowrap;  
                            } 
                            li{  text-align: center;
                                 @media(min-width:$screen-lg-min){
                                     width:16.666%; 
                                 }
                                 @media(max-width:$screen-xs-max){display: inline-block; float: none; width: auto;}
                                 &.active{
                                     a{
                                         background-color: transparent; color: $color-green; border:none; position: relative;
                                         &:after{
                                             content: ''; height: 2px; background-color: $color-green; bottom: 0; position: absolute; width: 100%; left: 0; right: 0;
                                         }
                                     }  
                                 }
                                 a{ 
                                     color: $color-666; font-size: 18px; color:$color-666;font-family: $avantgarde-md; padding: 15px 20px; border:none;
                                     @media(max-width:$screen-lg-max){
                                         font-size: 16px;padding: 15px 10px;  
                                     }
                                     @media(max-width:$screen-xs-max){
                                         font-size: 16px;padding: 15px 10px;  
                                     }
                                     &:hover{
                                         background-color: transparent; color: $color-green; border:none;
                                     }
                                 }
                            }
                       }
    }


    .box-width{
        @media(max-width:$screen-xs-max){
          width: 50%; float:left;
        }

        @media(max-width:480px){
          width: 100% !important;
        }
      }

    .tab-content{
        padding:40px 0 0px;
        @media(max-width:$screen-xs-max){
            padding: 20px 0 0px;
        }

        .complete-appointment-history{
            & > .row{
                margin: 0 -23px;
            }
            .list-box.online{
                .img-box{ border-color: $color-green;
                          .chat{background-image: url("#{$imageurl}/chat-icon-green.png"); }
                } 
            }
            
            .list-box{position: relative; margin: 0 8px 50px;text-align: center; padding: 60px 0px 0; background-color: $color-white;box-shadow: 0 0 3px -1px rgba(0, 0, 0, 0.2);
                      .rebook{ position: absolute; width:100%; padding: 19px 15px 0; top: 0;
                               .rebook-btn{
                                   display: block; float: left; border: 1px solid #F2C24C; color: #F8AD39; text-decoration: none;
                                   font-size: 16px; padding: 8px 15px; border-radius: 25px; font-family: $avantgarde-md;
                                   &:hover{border: 1px solid $color-green; color:  $color-green}

                                   @media (max-width:$screen-xs-max){
                                    font-size: 13px; padding: 6px 10px; 
                                   }

                                   @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){
                                    font-size: 14px; padding: 6px 11px; 
                                   }
                               }    
                               .right{ float: right; }
                      }
                      .img-box{ position: relative; height: 171px; width: 171px;margin: 0 auto; border:4px solid #A4A4A4;border-radius: 50%;

                          @media (max-width:$screen-xs-max){ width:120px;height:120px  }
                          @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){width:155px;height:155px;}


                                .provider-image{height: 100%; width: 100%; border-radius: 50%; border:5px solid $color-white;
                                                img{
                                                    height:100%; width: 100%; margin:0 auto;
                                                    @media (max-width:$screen-xs-max){
                                                      width:120px;
                                                   }
                                                   @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){width:155px;}
                                                }  
                                }

                                &.check{
                                    border-color:#F2C24C; 
                                }
                                .chat{
                                    display: inline-block; position: absolute; right: -8px; top: 15px; z-index: 999;
                                    height: 39px; width: 39px; background-image: url("../images/chat-icon-gray.png");
                                }
                                .wellza-member{
                                    position: absolute; right: 0; bottom: 16px;
                                }
                                .checkbox{display: inline-block; position: absolute; right:-11px; z-index: 999; top:15px;
                                          input[type=checkbox]:checked {
                                              + label{ 
                                                  span{ background-image: url('../images/like.png');  
                                                     @media(max-width:$screen-xs-max){
                                                      background-size:30px;
                                                     }
                                                  }
                                              }
                                          }
                                          label{ padding: 0px !important;
                                                 span{
                                                     background-image: url('../images/unlike.png'); height:40px; width: 40px;  background-repeat: no-repeat; 
                                                     display: inline-block; vertical-align: middle;
                                                     @media(max-width:$screen-xs-max){
                                                      height:30px; width: 30px; background-size:30px; 
                                                     }
                                                 }
                                          }
                                          .checkbox, .radio { width: 100%; margin:0;}
                                          input[type=checkbox] {display: none;}
                                          label{
                                              &:before{
                                                  content:"";display: inline-block;position:absolute;left: 0; background-color:#F5F7FA;      
                                              }
                                          }
                                          input[type=checkbox]:checked + label{  }

                                }

                      }
                      .provider-info{ margin: 10px 0;
                                      .provider-name{
                                          font-size: 28px; color: #333; margin-top: 15px;font-family:$avantgardestd-bk; 
                                          @media (max-width:$screen-xs-max){
                                              margin-top: 0px;font-size: 20px;
                                           }
                                           @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){margin-top: 0px;font-size: 22px;}
                                        }
                                      .address{ font-size: 18px; color: #A1A1A1;font-family:$avantgardestd-bk;
                                        @media (max-width:$screen-xs-max){font-size:14px;}
                                           @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){font-size:16px;}
                                      }
                                      .timing{ font-size: 18px; color: $color-pink;font-family:$avantgardestd-bk;margin:13px 0;
                                        @media (max-width:$screen-xs-max){ font-size:14px; }
                                        @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){font-size:16px;}   
                                      }
                                      .review{    font-size: 20px;    color: #A1A1A1;    font-family: $avantgardestd-bk;    font-weight: 600;    margin-top: 5px; 
                                                  @media (max-width:$screen-xs-max){font-size:16px;}
                                                  @media(min-width:$screen-sm-min) and (max-width:$screen-sm-max){font-size:18px;}
                                      }
                      }
                      .price-date-box{border-top: 2px solid #F0F0F0; letter-spacing: 0.9px; 
                                      .left, .right{
                                          float: left; width: 50%; text-align:left; padding: 10px 15px;
                                          @media(max-width:$screen-xs-max){
                                            width:100%; 
                                          }
                                         
                                          .icon{ margin-right: 10px;
                                                 img{
                                                      @media(max-width:$screen-xlg-max){width:30px;}
                                                 }
                                          }
                                          .icon, .price-box{
                                              float: left; 
                                          }
                                          .label-info{
                                              background-color: transparent; color: $color-666; font-size: 16px;
                                              @media (max-width:$screen-xs-max){
                                                 font-size:14px;
                                               }
                                          }
                                          .price, .time{
                                              font-size: 20px; color: $color-black;font-family: $avantgarde-md; 
                                              @media(max-width:$screen-xlg-max){
                                                font-size: 18px;
                                              }
                                          }
                                      }
                                      .left{
                                          border-right: 2px solid #F0F0F0;
                                           @media(max-width:$screen-xs-max){
                                            border-right:0;border-bottom: 2px solid #F0F0F0;
                                          }
                                      }
                      }

            }
        }

        // xxxxxxxxxxxxx //

