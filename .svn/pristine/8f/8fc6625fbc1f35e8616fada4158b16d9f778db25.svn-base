.admin-page{
    .appointment_section{
        .serviceproviderlist_slider{ background-color: #F9F9F9;
                                     .owl-stage-outer{ height: 110px;}
                                     .owl-item{
                                         &.center{ background-color:$color-white;}
                                         .item{text-align: center; padding: 20px 0px;
                                               .img_box{ padding:2px;height: 44px; width: 44px; border-radius: 50%;border: 2px solid #CDCDCD; border-radius: 50%; margin:0 auto; 
                                                         img{
                                                             border-radius: 50%;
                                                         }
                                               }
                                               .name{ font-size: 16px; color: $color-666; font-family: $avantgarde-md; margin-top: 5px;}

                                         } 
                                     }

                                     .owl-nav{ position: absolute; top: 37px; width: 99%; height: 0; margin: 0 auto; left: 0; right:0;
                                               .owl-prev, .owl-next{background: transparent; padding: 0; margin:0;}
                                               .owl-prev{ float: left; }
                                               .owl-next{ float: right;}
                                     }
        }
    }

}

