div#preloader { position: fixed; left: 0; top: 0; z-index: 999; width: 100%; height: 100%; overflow: visible; background-color: rgba(0, 0, 0, 0.5);}
div#preloader img {
    display: block;
    margin-left: auto;
    margin-right: auto;
    margin-top: 20%;
}
